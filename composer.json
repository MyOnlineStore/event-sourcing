{
    "name": "myonlinestore/event-sourcing",
    "type": "library",
    "description": "MyOnlineStore Event Sourcing base functionality",
    "homepage": "https://github.com/MyOnlineStore/event-sourcing",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\EventSourcing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\EventSourcing\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "beberlei/assert": "^3.2.3",
        "doctrine/dbal": "^2.9.2",
        "psr/event-dispatcher": "^1.0.0",
        "ramsey/uuid": "^3.8.0",
        "zendframework/zend-crypt": "^3.3.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "myonlinestore/coding-standard": "^1.1.2",
        "phpunit/phpunit": "^8.3.4",
        "vimeo/psalm": "^3.4.11"
    }
}
